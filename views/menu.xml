
<odoo>
    <menuitem id="menu_custom_iframe_top" name="Vicidial" sequence="10" web_icon="vicidial,static/description/icon.png" groups="base.group_user"/>
    <record id="action_custom_iframe_backend" model="ir.actions.server">
        <field name="name">Vicidial</field>
        <field name="model_id" ref="vicidial.model_custom_iframe"/>
        <field name="state">code</field>
        <field name="code">
            action = env['custom.iframe'].action_custom_iframe_backend()
        </field>
    </record>

    <menuitem id="menu_custom_iframe_backend"
              name="Vicidial"
              parent="menu_custom_iframe_top"
              action="vicidial.action_custom_iframe_backend"
              sequence="1"
              groups="base.group_user"/>
</odoo>
